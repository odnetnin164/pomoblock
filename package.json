{
  "name": "pomoblock-extension",
  "version": "1.0.0",
  "description": "A Chrome extension for website blocking with modular architecture",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "clean": "rimraf dist",
    "build-script": "node scripts/build.js",
    "package": "node scripts/build.js build && node scripts/build.js zip",
    "start": "node scripts/build.js watch"
  },
  "keywords": [
    "chrome-extension",
    "website-blocker",
    "productivity",
    "focus",
    "webpack",
    "modular"
  ],
  "author": "PomoBlock Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/pomoblock-extension.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/pomoblock-extension/issues"
  },
  "homepage": "https://github.com/your-username/pomoblock-extension#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}